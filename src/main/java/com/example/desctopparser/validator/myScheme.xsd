            <xsd:element name="PAYGRNDPARAM" type="xsd:string"/>
            <xsd:element name="PAYTYPEPARAM" type="xsd:string"/>
            <xsd:element name="PAYUNTIL" type="xsd:string"/>
            <xsd:element name="PAYER" type="xsd:string"/>
            <xsd:element name="PAYERACCOUNT" type="xsd:string"/>
            <xsd:element name="PAYERBIC" type="xsd:string"/>
            <xsd:element name="PAYERBANKNAME" type="xsd:string"/>
            <xsd:element name="PAYERBANKTYPE" type="xsd:string"/>
            <xsd:element name="PAYERCORRACCOUNT" type="xsd:string"/>
            <xsd:element name="PAYERINN" type="xsd:string"/>
            <xsd:element name="PAYERKPP" type="xsd:string"/>
            <xsd:element name="PAYERPLACE" type="xsd:string"/>
            <xsd:element name="PAYERPLACETYPE" type="xsd:string"/>
            <xsd:element name="PAYERPROPERTYTYPE" type="xsd:string"/>
            <xsd:element name="PAYMENTURGENT" type="xsd:string"/>
            <xsd:element name="RECEIVER" type="xsd:string"/>
            <xsd:element name="RECEIVERACCOUNT" type="xsd:string"/>
            <xsd:element name="RECEIVERBIC" type="xsd:string"/>
            <xsd:element name="RECEIVERBANKNAME" type="xsd:string"/>
            <xsd:element name="RECEIVERBANKTYPE" type="xsd:string"/>
            <xsd:element name="RECEIVERCORRACCOUNT" type="xsd:string"/>
