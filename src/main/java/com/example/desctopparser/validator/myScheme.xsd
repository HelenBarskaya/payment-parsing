<?xml version="1.0"?>
<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:ns2="http://www.vtb.ru/DBO20/IntegrationPlatform" targetNamespace="http://www.vtb.ru/DBO20/IntegrationPlatform" elementFormDefault="qualified">

    <xsd:element name="PayDocRuReq">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="BSHead" type="ns2:BSHeadType"/>
                <xsd:element name="BSRequest" type="ns2:BSRequestType"/>
            </xsd:sequence>
            <xsd:attribute name="Version" type="xsd:string" use="required"/>
            <xsd:attribute name="ID" type="xsd:string" use="required"/>
            <xsd:attribute name="DateTime" type="xsd:string" use="required"/>
        </xsd:complexType>
    </xsd:element>

    <xsd:complexType name="BSHeadType">
        <xsd:sequence>
            <xsd:element name="Client" type="ns2:ClientType"/>
            <xsd:element name="Branch" type="ns2:BranchType"/>
            <xsd:element name="Office" type="ns2:OfficeType"/>
        </xsd:sequence>
        <xsd:attribute name="RSys" type="xsd:string" use="required"/>
        <xsd:attribute name="ASys" type="xsd:string" use="required"/>
        <xsd:attribute name="route" type="xsd:string" use="required"/>
    </xsd:complexType>

    <xsd:complexType name="ClientType">
        <xsd:attribute name="RCustID" type="xsd:string" use="required"/>
        <xsd:attribute name="ACustID" type="xsd:string" use="required"/>
        <xsd:attribute name="INN" type="xsd:string" use="required"/>
    </xsd:complexType>

    <xsd:complexType name="BranchType">
        <xsd:attribute name="RBranchID" type="xsd:string" use="required"/>
        <xsd:attribute name="ABranchID" type="xsd:string" use="required"/>
        <xsd:attribute name="BIC" type="xsd:string" use="required"/>
    </xsd:complexType>

    <xsd:complexType name="OfficeType">
        <xsd:attribute name="Officecode" type="xsd:string" use="required"/>
        <xsd:attribute name="Robotuser" type="xsd:string" use="required"/>
    </xsd:complexType>

    <xsd:complexType name="BSRequestType">
        <xsd:sequence>
            <xsd:element name="AMOUNT" type="xsd:decimal"/>
            <xsd:element name="CBCCODE" type="xsd:string"/>
            <xsd:element name="CURRCODE" type="xsd:string"/>
            <xsd:element name="CUSTID" type="xsd:string"/>
